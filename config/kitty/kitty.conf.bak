font_family      mononoki Nerd Font Regular
bold_font        mononoki Nerd Font Bold
italic_font      mononoki Nerd Font Italic
bold_italic_font mononoki Nerd Font Bold Italic
bold_font auto
italic_font auto
bold_italic_font auto
font_size 15.0

# Let vim talk to kitty
listen_on unix:/tmp/kitty
allow_remote_control socket-only

# Keyboard shortcuts

map cmd+t     new_tab_with_cwd
map cmd+,     set_tab_title

# Theme
include tokyonight.conf



# BEGIN_KITTY_THEME
# YsDark
include current-theme.conf
# END_KITTY_THEME